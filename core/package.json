{
  "name": "@vbait/core",
  "version": "0.0.1",
  "private": false,
  "description": "Stencil Component Starter",
  "homepage": "https://vbait.github.io/acb-components/",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "jsnext:main": "dist/esm/index.js",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "types": "dist/types/index.d.ts",
  "unpkg": "dist/vbait/vbait.esm.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/vbait/acb-components"
  },
  "files": [
    "components/",
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "npm run clean && cross-env NODE_ENV=production stencil build --docs-json dist/docs.json",
    "clean": "node scripts/clean.js",
    "start": "cross-env NODE_ENV=development stencil build --dev --watch --serve",
    "test": "npm run test.spec && npm run test.e2e",
    "test.spec": "stencil test --spec",
    "test.e2e": "stencil test --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "eslint": "./node_modules/.bin/eslint src/**/*{.ts,.tsx}",
    "lint": "npm run lint.ts && npm run lint.sass && npm run prettier -- --write --cache",
    "lint.fix": "npm run lint.ts.fix && npm run lint.sass.fix && npm run prettier -- --write --cache",
    "lint.sass": "stylelint \"src/**/*.scss\"",
    "lint.sass.fix": "npm run lint.sass -- --fix",
    "lint.ts": "npm run eslint",
    "lint.ts.fix": "npm run eslint -- --fix",
    "prettier": "./node_modules/.bin/prettier \"./src/**/*.{html,ts,tsx,js,jsx}\"",
    "generate": "stencil generate",
    "sb": "start-storybook -p 6006",
    "sb.build": "build-storybook",
    "sb.deploy": "gh-pages -d storybook-static"
  },
  "dependencies": {
    "@stencil/core": "^3.0.0"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "@ionic/core": "^6.5.3",
    "@realpage/raul3": "^0.36.18"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@stencil-community/eslint-plugin": "^0.5.0",
    "@stencil/react-output-target": "^0.4.0",
    "@stencil/sass": "^2.0.3",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/builder-webpack5": "^6.5.16",
    "@storybook/html": "^6.5.16",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/testing-library": "0.0.13",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.12",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "babel-loader": "^8.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.33.0",
    "eslint-plugin-react": "^7.32.2",
    "gh-pages": "^5.0.0",
    "jest": "^27.5.1",
    "jest-cli": "^27.5.1",
    "moment": "^2.24.0",
    "prettier": "^2.8.4",
    "puppeteer": "^19.5.2",
    "stylelint": "^14.16.1",
    "stylelint-order": "^6.0.1",
    "typescript": "^4.0.5"
  },
  "license": "MIT",
  "jest": {
    "preset": "@stencil/core/testing"
  }
}
